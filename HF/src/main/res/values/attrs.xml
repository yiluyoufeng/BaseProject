<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClipViewLayout">
        <attr name="clipType" format="enum">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="mHorizontalPadding" format="dimension" />
        <attr name="clipBorderWidth" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="TabRadioButton">
        <attr name="text" format="string" />
        <attr name="textdimension" format="dimension" />
        <attr name="normalinsidebackground" format="reference" />
        <attr name="normalexternalbackground" format="reference" />
        <attr name="selectedinsidebackground" format="reference" />
        <attr name="selectedexternalbackground" format="reference" />
        
    </declare-styleable>
</resources>